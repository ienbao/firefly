<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.dmsoft.firefly.plugin.yield.controller.YieldMainController">
    <children>
        <SplitPane dividerPositions="1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <items>
                <VBox>
                    <GridPane maxHeight="28.0" minHeight="28.0" prefHeight="28.0" style="-fx-background-color: #ffffff;"
                              BorderPane.alignment="CENTER">
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="-1" minWidth="10.0" prefWidth="-1"/>
                            <ColumnConstraints hgrow="NEVER" maxWidth="25.0" minWidth="25.0" prefWidth="25.0"/>
                            <ColumnConstraints hgrow="NEVER" maxWidth="25.0" minWidth="25.0" prefWidth="25.0"/>
                            <ColumnConstraints hgrow="NEVER" maxWidth="25.0" minWidth="25.0" prefWidth="25.0"/>
                            <ColumnConstraints hgrow="NEVER" maxWidth="25.0" minWidth="25.0" prefWidth="25.0"/>
                        </columnConstraints>
                        <rowConstraints>
                            <RowConstraints minHeight="3.0" prefHeight="3.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="22.0" prefHeight="22.0" vgrow="SOMETIMES"/>
                            <RowConstraints minHeight="3.0" prefHeight="3.0" vgrow="SOMETIMES"/>
                        </rowConstraints>
                        <children>
                            <Label prefHeight="15.0" prefWidth="174.0" text="%YIELD_ANALYSIS_RESULT_TITLE"
                                   GridPane.rowIndex="1">
                                <padding>
                                    <Insets left="10.0"/>
                                </padding>
                            </Label>
                            <Button fx:id="printBtn" maxHeight="22.0" maxWidth="22.0" minWidth="22.0"
                                    mnemonicParsing="false" prefHeight="22.0" prefWidth="22.0" styleClass="btn-icon"
                                    GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                            <Button fx:id="exportBtn" maxHeight="22.0" maxWidth="22.0" minWidth="22.0"
                                    mnemonicParsing="false" prefHeight="22.0" prefWidth="22.0" styleClass="btn-icon"
                                    GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                            <Button fx:id="resetBtn" maxHeight="22.0" maxWidth="22.0" minWidth="22.0"
                                    mnemonicParsing="false" prefHeight="22.0" prefWidth="22.0" styleClass="btn-icon"
                                    GridPane.columnIndex="3" GridPane.rowIndex="1"/>
                            <Button fx:id="chooseBtn" maxHeight="22.0" maxWidth="22.0" minWidth="22.0"
                                    mnemonicParsing="false" prefHeight="22.0" prefWidth="22.0" styleClass="btn-primary"
                                    GridPane.columnIndex="4" GridPane.rowIndex="1"/>
                        </children>
                    </GridPane>

                    <SplitPane dividerPositions="0.35181975736568455, 0.6825633383010432" layoutY="28.0"
                               orientation="VERTICAL" prefWidth="1210.0" VBox.vgrow="ALWAYS">
                        <items>
                            <fx:include fx:id="yieldResult" source="yield_result_chart.fxml"/>
                            <fx:include fx:id="overView" source="over_view.fxml"/>
                            <fx:include fx:id="viewData" source="view_data.fxml"/>
                        </items>
                    </SplitPane>
                </VBox>
                <VBox minWidth="180.0" prefWidth="180.0" SplitPane.resizableWithParent="false">
                    <fx:include fx:id="yieldItem" source="yield_item.fxml" VBox.vgrow="ALWAYS"/>
                </VBox>
            </items>
        </SplitPane>
    </children>
</AnchorPane>
