<?xml version="1.0" encoding="UTF-8"?>

<?import com.dmsoft.firefly.gui.components.utils.TextFieldFilter?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minHeight="110.0" minWidth="180.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dmsoft.firefly.plugin.yield.controller.ViewDataController">
    <children>
        <TabPane style="-fx-border-width: 0 0 0 0;" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <tabs>
                <Tab text="%VIEW_DATA">
                    <content>
                        <VBox fx:id="vbox" minHeight="0.0" minWidth="0.0" >
                            <HBox prefHeight="30" alignment="CENTER" prefWidth="390" AnchorPane.bottomAnchor="390.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                <HBox  minWidth="10.0" prefWidth="386.0" style="-fx-padding: 3 0 3 0;">
                                    <children>
                                        <Label fx:id="viewDataR" />
                                        <Label fx:id="viewDataC" />
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_RIGHT" prefHeight="30" minWidth="700" prefWidth="880" style="-fx-spacing:5;-fx-padding: 3 3 3 0">
                                    <TextFieldFilter fx:id="filteValueTf" maxWidth="200.0"  prefHeight="24.0"  prefWidth="170.0" >
                                        <GridPane.margin>
                                            <Insets bottom="3.0" right="1.0" top="6.0" />
                                        </GridPane.margin>
                                        <opaqueInsets>
                                            <Insets />
                                        </opaqueInsets>
                                    </TextFieldFilter>
                                    <Button fx:id="chooseColumnBtn" maxHeight="22.0" maxWidth="22.0" minWidth="22.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="22.0" style="-fx-background-image: url('images/btn_choose_test_items_normal.png'); -fx-background-size: 16; " styleClass="btn-icon"  />
                                </HBox>
                            </HBox>
                            <TableView fx:id="viewDataTable" style="-fx-border-width: 1 0 0 0;" BorderPane.alignment="CENTER" VBox.vgrow="ALWAYS">
                            </TableView>
                        </VBox>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
