<?xml version="1.0" encoding="UTF-8"?>

<?import com.dmsoft.firefly.gui.components.utils.TextFieldFilter?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane minHeight="110.0" minWidth="180.0" style="-fx-background-color: #ffffff;"
            xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.dmsoft.firefly.plugin.yield.controller.ViewDataController">
    <children>
        <TabPane style="-fx-border-width: 0 0 0 0;" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0"
                 AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <tabs>
                <Tab text="%VIEW_DATA">
                    <content>
                        <VBox fx:id="vbox" minHeight="0.0" minWidth="0.0">
                            <GridPane AnchorPane.bottomAnchor="242.0" AnchorPane.rightAnchor="0.0"
                                      AnchorPane.topAnchor="0.0" BorderPane.alignment="CENTER">
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="210.0" minWidth="20.0"
                                                       prefWidth="210.0"/>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="70.0" minWidth="70.0"
                                                       prefWidth="70.0"/>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="25.0" minWidth="25.0"
                                                       prefWidth="25.0"/>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="25.0" minWidth="25.0"
                                                       prefWidth="25.0"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints maxHeight="28.0" minHeight="28.0" prefHeight="28.0"
                                                    vgrow="SOMETIMES"/>
                                </rowConstraints>
                                <children>
                                    <TextFieldFilter fx:id="filteValueTf" maxWidth="200.0" prefWidth="200.0"
                                                     GridPane.columnIndex="1">
                                        <GridPane.margin>
                                            <Insets bottom="3.0" right="10.0" top="3.0"/>
                                        </GridPane.margin>
                                        <opaqueInsets>
                                            <Insets/>
                                        </opaqueInsets>
                                    </TextFieldFilter>
                                    <Button fx:id="chooseColumnBtn" maxHeight="22.0" maxWidth="22.0" minWidth="22.0"
                                            mnemonicParsing="false" prefHeight="22.0" prefWidth="22.0"
                                            style="-fx-background-image: url('images/btn_choose_test_items_normal.png'); -fx-background-size: 16;"
                                            styleClass="btn-icon" GridPane.columnIndex="4"/>
                                </children>
                            </GridPane>
                            <TableView fx:id="viewDataTable" style="-fx-border-width: 1 0 0 0;"
                                       BorderPane.alignment="CENTER" VBox.vgrow="ALWAYS">
                                <!--<columns>-->
                                <!--<TableColumn fx:id="checkBoxColumn" maxWidth="30.0" minWidth="30.0" prefWidth="30" resizable="false" sortable="false" />-->
                                <!--</columns>-->
                            </TableView>
                        </VBox>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
