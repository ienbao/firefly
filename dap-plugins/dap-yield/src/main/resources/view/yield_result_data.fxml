<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.dmsoft.firefly.plugin.yield.controller.YieldResultDataController">
    <children>
        <Label text="Total Samples" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
        <Label text="FPY Samples" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <Label text="Pass Samples" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
        <Label text="NTF Samples" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
        <Label text="NG Samples" GridPane.columnIndex="0" GridPane.rowIndex="4"/>

        <Label fx:id="TotalSamples" textAlignment="RIGHT" alignment="CENTER_RIGHT" text="-" GridPane.columnIndex="1"
               GridPane.rowIndex="0"/>
        <Label fx:id="FpySamples" textAlignment="RIGHT" alignment="CENTER_RIGHT" text="-" GridPane.columnIndex="1"
               GridPane.rowIndex="1"/>
        <Label fx:id="PassSamples" textAlignment="RIGHT" alignment="CENTER_RIGHT" text="-" GridPane.columnIndex="1"
               GridPane.rowIndex="2"/>
        <Label fx:id="NtfSamples" textAlignment="RIGHT" alignment="CENTER_RIGHT" text="-" GridPane.columnIndex="1"
               GridPane.rowIndex="3"/>
        <Label fx:id="NgSamples" textAlignment="RIGHT" alignment="CENTER_RIGHT" text="-" GridPane.columnIndex="1"
               GridPane.rowIndex="4"/>
    </children>
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="32.0" minHeight="32.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="32.0" minHeight="32.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="32.0" minHeight="32.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="32.0" minHeight="32.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="32.0" minHeight="32.0" vgrow="SOMETIMES"/>
    </rowConstraints>
</GridPane>
