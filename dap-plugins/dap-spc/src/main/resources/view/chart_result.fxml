<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="110.0" minWidth="309.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.dmsoft.firefly.plugin.spc.controller.ChartResultController">
    <TabPane  tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0"
             AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" styleClass="spc-chart-tab">
        <tabs>
            <Tab text="%ANALYSIS_RESULT" >
                <TabPane fx:id="analysisChartTabPane" rotateGraphic="true" side="LEFT"
                         styleClass="vertical-text,spc-chart-tab"
                         tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab fx:id="ndTab" closable="false">
                            <graphic>
                                <Group>
                                    <Label styleClass="tab-graphic-label" text="%SPC_CHART_NDC"
                                           textAlignment="CENTER"/>
                                </Group>
                            </graphic>
                        </Tab>
                        <Tab fx:id="runTab" closable="false">
                            <graphic>
                                <Group>
                                    <Label rotate="90" styleClass="tab-graphic-label" text="%SPC_CHART_RUN"
                                           textAlignment="CENTER"/>
                                </Group>
                            </graphic>
                        </Tab>
                        <Tab closable="false" fx:id="xBarTab">
                            <graphic>
                                <Group>
                                    <Label styleClass="tab-graphic-label" text="%SPC_CHART_BAR"
                                           textAlignment="CENTER"/>
                                </Group>
                            </graphic>
                        </Tab>
                        <Tab fx:id="rangeTab" closable="false">
                            <graphic>
                                <Group>
                                    <Label styleClass="tab-graphic-label" text="%SPC_CHART_RANGE"
                                           textAlignment="CENTER"/>
                                </Group>
                            </graphic>
                        </Tab>
                        <Tab fx:id="sdTab" closable="false">
                            <graphic>
                                <Group>
                                    <Label styleClass="tab-graphic-label" text="%SPC_CHART_SD"
                                           textAlignment="CENTER"/>
                                </Group>
                            </graphic>
                        </Tab>
                        <Tab fx:id="medianTab" closable="false">
                            <graphic>
                                <Group>
                                    <Label styleClass="tab-graphic-label" text="%SPC_CHART_MEDIAN"
                                           textAlignment="CENTER"/>
                                </Group>
                            </graphic>
                        </Tab>
                        <Tab fx:id="boxTab" closable="false">
                            <graphic>
                                <Group>
                                    <Label styleClass="tab-graphic-label" text="%SPC_CHART_BOX"
                                           textAlignment="CENTER"/>
                                </Group>
                            </graphic>
                        </Tab>
                        <Tab fx:id="mrTab" closable="false">
                            <graphic>
                                <Group>
                                    <Label styleClass="tab-graphic-label" text="%SPC_CHART_MR"
                                           textAlignment="CENTER"/>
                                </Group>
                            </graphic>
                        </Tab>
                    </tabs>
                </TabPane>
            </Tab>
        </tabs>
    </TabPane>
</AnchorPane>
