<?xml version="1.0" encoding="UTF-8"?>

<?import com.dmsoft.firefly.gui.components.utils.TextFieldFilter?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="800.0"
          xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.dmsoft.firefly.plugin.grr.controller.GrrViewDataController">
    <columnConstraints>
        <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="225.0" minWidth="225.0" prefWidth="225.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="28.0" minHeight="28.0" prefHeight="28.0" valignment="CENTER" vgrow="NEVER"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" valignment="CENTER" vgrow="NEVER"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="42.0" minHeight="42.0" prefHeight="42.0" vgrow="NEVER"/>
    </rowConstraints>
    <children>
        <Button fx:id="exchangeBtn" mnemonicParsing="false" styleClass="btn-primary" text="%EXCHANGE"
                GridPane.rowIndex="4">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <GridPane.margin>
                <Insets left="10.0"/>
            </GridPane.margin>
        </Button>
        <Label text="%ANALYSIS_DATA">
            <padding>
                <Insets left="10.0"/>
            </padding>
        </Label>
        <TableView fx:id="analysisDataTB" prefHeight="200.0" prefWidth="200.0"
                   GridPane.columnSpan="2" GridPane.rowIndex="1"/>
        <TableView fx:id="exchangeDataTB" prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="2"
                   GridPane.rowIndex="3"/>
        <TextFieldFilter fx:id="analysisFilterLB" prefHeight="22.0"
                         GridPane.columnIndex="1">
            <GridPane.margin>
                <Insets bottom="3.0" right="3.0" top="3.0"/>
            </GridPane.margin>
        </TextFieldFilter>
        <TextFieldFilter fx:id="exchangeFilterLB" prefHeight="22.0"
                         GridPane.columnIndex="1"
                         GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets bottom="3.0" right="3.0" top="3.0"/>
            </GridPane.margin>
        </TextFieldFilter>
        <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="2">
            <children>
                <Label text="%EXCHANGEABLE_DATA">
                    <HBox.margin>
                        <Insets left="10.0"/>
                    </HBox.margin>
                </Label>
                <Label fx:id="exchangeableLB">
                    <HBox.margin>
                        <Insets left="10.0"/>
                    </HBox.margin>
                </Label>
            </children>
        </HBox>
    </children>
</GridPane>
