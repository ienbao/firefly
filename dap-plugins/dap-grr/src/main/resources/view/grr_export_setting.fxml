            </RadioButton>
            <RadioButton fx:id="system" mnemonicParsing="false" text="%EXPORT_SYSTEM">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </RadioButton>
         </children>
         <padding>
            <Insets left="20.0" />
         </padding>
      </VBox>
      <HBox alignment="CENTER_RIGHT" prefHeight="45.0" prefWidth="200.0">
