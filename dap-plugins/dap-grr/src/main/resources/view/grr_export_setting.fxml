                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <CheckBox fx:id="rrPart" mnemonicParsing="false" text="%CHART_1" GridPane.columnIndex="1" />
                  <CheckBox fx:id="rrAppraiser" mnemonicParsing="false" text="%CHART_2" GridPane.columnIndex="2" />
                  <CheckBox fx:id="rc" mnemonicParsing="false" text="%CHART_3" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <CheckBox mnemonicParsing="false" text="%CHART_4" GridPane.columnIndex="2" GridPane.rowIndex="1" fx:id="xbar" />
                  <CheckBox fx:id="pai" mnemonicParsing="false" text="%CHART_5" GridPane.columnIndex="1" GridPane.rowIndex="2" />
