<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox styleClass="gui-export-vbox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dmsoft.firefly.gui.controller.ExportSettingController">
      <TableView fx:id="settingTable" styleClass="gui-export-table" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="selector" editable="false" resizable="false" styleClass="gui-export-selector" sortable="false" />
          <TableColumn fx:id="settings" editable="false"  resizable="false" styleClass="gui-export-setting" sortable="false" text="%EXPORT_SETTINGS" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
       <HBox styleClass="gui-export-hbox">
               <Button fx:id="exportBtn" styleClass="gui-export-btn" mnemonicParsing="false" text="%EXPORT_BTN" />
               <Button fx:id="cancelBtn" styleClass="gui-export-btn" mnemonicParsing="false" text="%CANCEL_BTN"/>
       </HBox>
</VBox>
