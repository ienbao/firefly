<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="158.0" minHeight="158.0" prefHeight="158.0" style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dmsoft.firefly.gui.controller.AboutController">
    <children>
          <HBox style="-fx-background-color: transparent;" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="18.0">
              <children>
                  <ImageView fx:id="imageViewLogo" fitHeight="90.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                      <HBox.margin>
                          <Insets right="40.0" />
                      </HBox.margin>
                  </ImageView>
                  <GridPane alignment="CENTER" maxHeight="90.0" vgap="10.0">
                      <columnConstraints>
                          <ColumnConstraints hgrow="SOMETIMES" percentWidth="0.0" />
                  <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" percentWidth="0.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
                      </columnConstraints>
                      <rowConstraints>
                          <RowConstraints percentHeight="0.0" valignment="CENTER" vgrow="SOMETIMES" />
                          <RowConstraints percentHeight="0.0" valignment="CENTER" vgrow="SOMETIMES" />
                          <RowConstraints percentHeight="0.0" vgrow="SOMETIMES" />
                      </rowConstraints>
                      <children>
                          <Label styleClass="login-label-lg" text="%GLOBAL_SYSTEM_PROCESS_TITLE" GridPane.columnSpan="3">
                              <font>
                                  <Font name="Arial" size="28.0" />
                              </font>
                          </Label>
                          <Label styleClass="login-label-md" text="%GLOBAL_SYSTEM_PROCESS_NAME" GridPane.columnSpan="3" GridPane.rowIndex="1">
                              <font>
                                  <Font name="Arial" size="22.0" />
                              </font>
                          </Label>
                          <Label fx:id="versionLabel" style="-fx-text-fill: #aaaaaa;" GridPane.rowIndex="2">
                              <font>
                                  <Font name="Arial" size="12.0" />
                              </font>
                          </Label>
                  <Label style="-fx-text-fill: #aaaaaa;" text="|" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <padding>
                        <Insets left="5.0" right="5.0" />
                     </padding>
                  </Label>
                  <Label fx:id="osLabel" style="-fx-text-fill: #aaaaaa;" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                      </children>
                  </GridPane>
              </children>
          </HBox>
    </children>
</AnchorPane>
