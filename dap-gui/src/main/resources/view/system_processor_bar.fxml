<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox styleClass="initialize-padding,systemProcessBar" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dmsoft.firefly.gui.SystemProcessorController">
    <HBox styleClass="initialize-h-box" >
        <ImageView fx:id="imageViewLogo" fitHeight="90.0" fitWidth="100.0"  />
        <GridPane maxWidth="360.0" minWidth="360.0" prefWidth="360.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="12.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="20.0" valignment="CENTER" vgrow="SOMETIMES"/>
            </rowConstraints>
                <Label styleClass="login-label-lg" text="%GLOBAL_SYSTEM_PROCESS_TITLE" GridPane.rowIndex="1"/>
                <Label styleClass="login-label-md" text="%GLOBAL_SYSTEM_PROCESS_NAME" GridPane.rowIndex="2"/>
                <ProgressBar fx:id="progressBar" maxHeight="2.0" maxWidth="350.0" minHeight="2.0" minWidth="350.0" prefHeight="2.0" prefWidth="350.0" progress="0.45" GridPane.rowIndex="3"/>
                <Label styleClass="login-label-sm" prefHeight="16.0" prefWidth="360.0" text="%GLOBAL_SYSTEM_PROCESS_INIT" GridPane.rowIndex="4"/>
                <Label styleClass="login-label-sm" alignment="CENTER_RIGHT" prefHeight="15.0" prefWidth="350.0" text="%STATE_BAR_VERSION" GridPane.rowIndex="5"/>
        </GridPane>
    </HBox>
</HBox>
