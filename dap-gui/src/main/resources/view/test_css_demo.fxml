<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.RadioButton?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">
  <HBox>
    <VBox style="-fx-spacing: 10.0">
      <Button text="测试按钮"></Button>
      <Button text="测试按钮" disable="true"></Button>
      <Button text="测试按钮" styleClass="btn-primary"></Button>
      <Button text="测试按钮" disable="true" styleClass="btn-primary"></Button>
      <CheckBox text="复选按钮" ></CheckBox>
      <CheckBox text="复选按钮" selected="true" disable="true" ></CheckBox>
      <RadioButton text="单选按钮"></RadioButton>
      <RadioButton text="单选按钮" selected="true" disable="true"></RadioButton>
      <Button styleClass="green-btn">
      </Button>
      <Button styleClass="green-btn-loading">
        <graphic>
          <ImageView styleClass="green-btn-loading-image" fitWidth="16.0" fitHeight="16.0" />
        </graphic>
      </Button>
      <Button styleClass="green-btn" disable="true"/>
    </VBox>

    <VBox style="-fx-spacing: 10.0">
      <TextField text = "文本"></TextField>
      <TextField text = "文本" disable="true"></TextField>
      <TextField styleClass="text-field-error"></TextField>
      <TextField styleClass="text-field-search"></TextField>
    </VBox>
  </HBox>
</AnchorPane>
