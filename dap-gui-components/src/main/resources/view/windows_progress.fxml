<VBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefHeight="20.0" prefWidth="200.0">
         <children>
            <Label text="Analyzing">
               <HBox.margin>
                  <Insets />
               </HBox.margin></Label>
         </children>
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="5.0" />
         </VBox.margin></HBox>
      <HBox prefHeight="10.0" prefWidth="200.0">
         <children>
            <ProgressBar fx:id="taskProgress" maxHeight="8.0" maxWidth="390.0" minHeight="8.0" minWidth="390.0" prefHeight="8.0" prefWidth="390.0" progress="0.0" HBox.hgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="5.0" />
         </VBox.margin></HBox>
      <HBox fx:id="hbox" VBox.vgrow="ALWAYS">
         <children>
            <TextArea fx:id="errorTxt" prefHeight="240.0" prefWidth="240.0" HBox.hgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="5.0" />
         </VBox.margin></HBox>
      <HBox alignment="TOP_RIGHT" prefHeight="20.0" prefWidth="200.0">
         <children>
            <Button fx:id="cancelBtn" mnemonicParsing="false" text="Button" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
         </VBox.margin></HBox>
   </children>
</VBox>
